{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "react-native-reanimated": ["./src"],
      "moduleSuffixes": [".web", ""]
    }
  },
  "include": ["src"],
  "exclude": [
    "src/**/*.native.ts",
    "src/**/*.native.tsx",
    "src/**/*.ios.ts",
    "src/**/*.ios.tsx",
    "src/**/*.android.ts",
    "src/**/*.android.tsx"
  ]
}
