{
  "name": "docs-worklets",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build && node -r esbuild-register scripts/build-og-images.jsx && mkdir -p ../docs-reanimated/build/react-native-worklets && mv build/* ../docs-reanimated/build/react-native-worklets",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --noEmit",
    "lint": "prettier --check docs src",
    "format": "prettier --write --list-different docs src"
  },
  "dependencies": {
    "@docusaurus/core": "3.7.0",
    "@docusaurus/plugin-debug": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mdx-js/react": "^3.0.0",
    "@mui/material": "^7.1.0",
    "@swmansion/t-rex-ui": "1.0.0",
    "@vercel/og": "^0.6.2",
    "clsx": "^2.1.0",
    "glob": "^10.4.2",
    "raf": "^3.4.1",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.71.4",
    "react-native-reanimated": "^4.0.0-nightly-20250325-d67e51599",
    "react-native-worklets": "^0.1.0",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "esbuild": "^0.25.0",
    "esbuild-register": "^3.5.0",
    "eslint-plugin-mdx": "^2.2.0",
    "prettier": "^2.8.4",
    "typescript": "~5.2.2",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "packageManager": "yarn@1.22.22"
}
