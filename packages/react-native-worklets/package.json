    "!**/__mocks__",
    "!**/.*"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": false,
          "jsxRuntime": "automatic"
        }
      ],
      "typescript"
    ]
  },
  "codegenConfig": {
    "name": "rnworklets",
    "type": "modules",
    "jsSrcsDir": "src/specs",
    "android": {
      "javaPackageName": "com.swmansion.worklets"
    },
    "ios": {
      "modulesConformingToProtocol": {
        "RCTBundleConsumer": [
          "WorkletsModule"
        ]
      }
    }
  }
