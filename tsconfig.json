{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "preserveSymlinks": true,
    "target": "es6",
    "module": "NodeNext",
    "jsx": "react-native",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "NodeNext",
    "lib": ["es6", "dom"],
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
