{
  "name": "next-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:analyze": "ANALYZE_BUNDLE=1 next build",
    "build:disable-minification": "DISABLE_MINIFICATION=1 next build",
    "start": "next start",
    "e2e": "start-server-and-test http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test http://localhost:3000 \"cypress run --e2e\"",
    "lint": "eslint --max-warnings=0 .",
    "format": "prettier --write --list-different ."
  },
  "dependencies": {
    "expo": "~48.0.10",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-reanimated": "workspace:*",
    "react-native-web": "^0.18.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/next-adapter": "^5.0.2",
    "@next/bundle-analyzer": "^13.5.3",
    "cypress": "^13.2.0",
    "eslint": "^8.57.0",
    "next-compose-plugins": "^2.2.1",
    "prettier": "2.8.8",
    "start-server-and-test": "^2.0.1"
  }
}
