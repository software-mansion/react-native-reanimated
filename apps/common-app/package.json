{
  "name": "common-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "",
    "lint": "eslint . --flag v10_config_lookup_from_file",
    "format": "prettier --write --list-different .",
    "circular-dependency-check": "yarn madge --extensions js,jsx,ts,tsx --circular src",
    "type:check": "yarn type:check:native && yarn type:check:web",
    "type:check:native": "tsc --noEmit",
    "type:check:web": "tsc --noEmit --project tsconfig.web.json",
    "type:check:strict": "tsc --noEmit --customConditions react-native-strict-api"
  },
  "peerDependencies": {
    "react-native": "*"
  },
  "dependencies": {
    "d3-shape": "3.2.0",
    "fuse.js": "patch:fuse.js@npm%3A7.1.0#~/.yarn/patches/fuse.js-npm-7.1.0-5dcae892a6.patch",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-native-reanimated": "workspace:*",
    "react-native-worklets": "workspace:*",
    "react-strict-dom": "0.0.54"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@babel/runtime": "7.28.4",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@tsconfig/react-native": "3.0.0",
    "@types/d3-shape": "3.1.7",
    "@types/jest": "30.0.0",
    "@types/react": "19.2.2",
    "@types/react-test-renderer": "19.1.0",
    "eslint": "9.37.0",
    "jest": "30.2.0",
    "madge": "8.0.0",
    "prettier": "3.6.2",
    "react-native": "0.82.0",
    "react-test-renderer": "19.1.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.46.0"
  }
}
